<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane prefHeight="1051.0" prefWidth="534.0">
            <children>
                <Button fx:id="logb" layoutX="145.0" layoutY="44.0" mnemonicParsing="false" onAction="#on_log_in" prefHeight="33.0" prefWidth="121.0" text="LOGIN USER" />
                <Button fx:id="signb" layoutX="285.0" layoutY="44.0" mnemonicParsing="false" onAction="#on_sign_in" prefHeight="33.0" prefWidth="121.0" text="SIGNUP" />
                <Button fx:id="logo" layoutX="6.0" layoutY="44.0" mnemonicParsing="false" onAction="#on_log_of" prefHeight="33.0" prefWidth="121.0" text="LOGIN OFFICIAL" />
                <LineChart fx:id="Vaccine_chart" layoutX="49.0" layoutY="113.0" prefHeight="183.0" prefWidth="314.0" title="User Vaccinated Per Day">
                    <xAxis>
                        <CategoryAxis label="Days" side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                        <NumberAxis label="People" side="LEFT" />
                    </yAxis>
                </LineChart>
                <ChoiceBox fx:id="TimeBox" layoutX="130.0" layoutY="283.0" prefWidth="150.0" />
                <Label layoutX="67.0" layoutY="287.0" prefHeight="17.0" prefWidth="48.0" text="Time" />
                <Button fx:id="initBT" layoutX="6.0" layoutY="87.0" mnemonicParsing="false" onAction="#Linit" prefHeight="33.0" prefWidth="121.0" text="Initialize" />
                <Button fx:id="getBT" layoutX="304.0" layoutY="284.0" mnemonicParsing="false" onAction="#BTget" text="GET" />
                <Button fx:id="signup_official_button" layoutX="290.0" layoutY="91.0" mnemonicParsing="false" onAction="#signup_official" text="SIGNUP_OFFICIAL" />
                <Label layoutX="155.0" layoutY="325.0" prefHeight="33.0" prefWidth="185.0" text="Cases Per Province" textAlignment="CENTER">
                    <font>
                        <Font name="Arial" size="17.0" />
                    </font>
                </Label>
            <TextArea fx:id="stateTA" layoutX="152.0" layoutY="837.0" prefHeight="200.0" prefWidth="215.0" />
            <Button fx:id="stateBT" layoutX="324.0" layoutY="806.0" mnemonicParsing="false" onAction="#stateOnClick" text="Filter" />
            <Label layoutX="205.0" layoutY="767.0" prefHeight="33.0" prefWidth="150.0" text="Filter One Province" />
            <TextField fx:id="stateTF" layoutX="132.0" layoutY="806.0" />
            <PieChart fx:id="txtbox" layoutX="-290.0" layoutY="354.0" prefHeight="405.0" prefWidth="1138.0" />
            </children>
        </Pane>
    </children>
</GridPane>
